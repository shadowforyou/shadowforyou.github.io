<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.poi.testpoi.mapper.HyYrqsFMapper">

	<select id="selectstcd" resultType="Integer" parameterType="com.poi.testpoi.pojo.HyYrqsF">
		select count(1) from hy_yrqs_f where STCD=#{stcd} and SDTP=#{sdtp} and YR=#{year}
	</select>
	
	<delete id="delete" parameterType="com.poi.testpoi.pojo.HyYrqsF">
		delete from hy_yrqs_f where
		STCD=#{stcd} and SDTP=#{sdtp} and YR=#{year}
	</delete>
	
	<insert id="add" parameterType="com.poi.testpoi.pojo.HyYrqsF">
        INSERT INTO hy_yrqs_f (STCD,SDTP,YR,AVQS,AVQSRCD,MXDYQS,MXDYQSRCD,MXDYQSODT,SW,SWRC,SM,NT) 
        VALUES (#{stcd},#{sdtp},#{year},#{avqs},#{avqsrcd},#{mxdyqs},#{mxdyqsrcd},#{mxdyqsodt},#{sw},#{swrc},#{sm},#{nt})
    </insert>

</mapper>